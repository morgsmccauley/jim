{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "prefer-function-over-method": true,
    "import-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-boolean-literal-compare": false,
    "max-line-length": [true, 120]
  },
  "rulesDirectory": []
}
